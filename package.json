{
  "name": "skeet-graphql",
  "version": "1.7.0",
  "description": "Skeet Framework Boilerplate with Next.js and GraphQL",
  "repository": "https://github.com/elsoul/skeet-graphql.git",
  "author": "ELSOUL LABO B.V.",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "format": "prettier --write --ignore-unknown .",
    "lint": "eslint --ext .ts,.tsx --fix .",
    "skeet": "run-p skeet:*",
    "skeet:graphql": "yarn --cwd ./graphql dev",
    "skeet:skeet": "yarn --cwd functions/skeet dev",
    "skeet:dev": "firebase emulators:start --import=./tmp/data --export-on-exit",
    "skeet:webapp": "yarn dev",
    "functions:run": "run-p functions:*",
    "functions:skeet": "yarn --cwd functions/skeet dev",
    "functions:dev": "firebase emulators:start --import=./tmp/data --export-on-exit",
    "backend": "run-p backend:*",
    "backend:build": "yarn functions:build",
    "backend:run": "yarn functions:dev",
    "fb:login": "cd functions/skeet && npx ts-node -r tsconfig-paths/register --transpile-only ./src/lib/login.ts",
    "update:packages": "ncu -u -x 'node-fetch,glob,remark,unified,remark-gfm,remark-rehype,remark-stringify,rehype-stringify,remark-parse,remark-directive' && yarn",
    "db:deploy": "yarn --cwd ./graphql db:deploy",
    "db:seed": "yarn --cwd ./graphql db:seed",
    "dev": "run-p 'dev:*'",
    "dev:relay": "relay-compiler --watch",
    "dev:next": "next dev -p 4200",
    "build:production:webapp": "yarn build",
    "build": "relay-compiler && NODE_ENV=production next build",
    "postbuild": "npx ts-node -r tsconfig-paths/register --transpile-only ./scripts/generateSitemap.ts",
    "deploy": "yarn build && npx firebase deploy --only hosting",
    "deploy:rules": "npx firebase deploy --only firestore:rules,storage",
    "check:webapp": "npx serve web-build",
    "send:sitemap": "npx ts-node -r tsconfig-paths/register --transpile-only ./scripts/sendSitemap.ts",
    "docker:build": "cd ./graphql && docker build -f Dockerfile.dev -t skeet-graphql .",
    "docker:run": "docker run -p 3000:3000 --env-file ./graphql/.env -d --rm --name skeet-graphql skeet-graphql",
    "docker:rm": "docker rm -f skeet-graphql",
    "discord": "npx ts-node -r tsconfig-paths/register --transpile-only ./.github/workflows/runDiscordChangeLog.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.4.2",
    "@fortawesome/free-brands-svg-icons": "6.4.2",
    "@fortawesome/free-regular-svg-icons": "6.4.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.0.18",
    "@hookform/resolvers": "3.3.2",
    "clsx": "2.0.0",
    "date-fns": "2.30.0",
    "dotenv": "16.3.1",
    "firebase": "10.5.2",
    "highlight.js": "11.9.0",
    "i18next": "23.6.0",
    "lodash.throttle": "4.1.1",
    "markdown-to-txt": "2.0.1",
    "next": "14.0.0",
    "next-i18next": "14.0.3",
    "next-language-detector": "1.0.2",
    "next-pwa": "5.6.0",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-hook-form": "7.47.0",
    "react-i18next": "13.3.1",
    "react-relay": "16.0.0",
    "react-relay-network-modern": "6.2.2",
    "recoil": "0.7.7",
    "recoil-persist": "5.1.0",
    "regenerator-runtime": "0.14.0",
    "relay-runtime": "16.0.0",
    "text-encoding": "0.7.0",
    "zod": "3.22.4"
  },
