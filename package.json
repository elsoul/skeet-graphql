    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "jest": "29.6.1",
    "nodemon": "3.0.1",
    "npm-check-updates": "16.10.16",
    "npm-run-all": "4.1.5",
    "prettier": "3.0.0",
    "prettier-plugin-prisma": "4.17.0",
    "superagent": "8.0.9",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
